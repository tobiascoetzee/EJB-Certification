<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>StatefulServlet</servlet-name>
        <servlet-class>com.tobiascode.client.StatefulServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StatelessServlet</servlet-name>
        <servlet-class>com.tobiascode.client.StatelessServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StatefulServlet</servlet-name>
        <url-pattern>/stateful.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StatelessServlet</servlet-name>
        <url-pattern>/stateless.do</url-pattern>
    </servlet-mapping>
</web-app>
